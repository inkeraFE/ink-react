{
  "name": "ink-rui",
  "version": "0.4.3",
  "description": "A React UI Toolkit for Mobile",
  "main": "dist/ink.cjs.js",
  "module": "dist/ink.es.js",
  "unpkg": "dist/ink.umd.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ./packages; npm run lintCss",
    "start": "cross-env NODE_ENV=development rollup -c ./script/rollup.config.js --watch",
    "build": "cross-env NODE_ENV=production rollup -c ./script/rollup.config.js; npm run buildLib; npm run buildCss",
    "buildLib": "babel packages --out-dir lib",
    "buildCss": "postcss packages/**/*.less --base packages --dir lib",
    "lintCss": "stylelint 'packages/**/*.less' --fix"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie > 8",
    "iOS >= 7",
    "Android >= 4.4"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:inkeraFE/ink-react.git"
  },
  "keywords": [
    "ink",
    "design",
    "React-component",
    "React",
    "ui",
    "component"
  ],
  "author": "inkera",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-jsx": "^7.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-standard": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "less": "^3.9.0",
    "postcss": "^7.0.17",
    "postcss-cli": "^5.0.1",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-simple-vars": "^5.0.2",
    "precss": "^3.1.2",
    "prettier": "^1.18.2",
    "rollup": "^1.16.6",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0"
  }
}
